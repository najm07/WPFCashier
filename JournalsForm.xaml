                        <TextBox Name="SupplierAmountTextBox" Text="{Binding ElementName=SupplierItemList ,Path=SelectedItem.Amount,Mode=OneWay}" HorizontalAlignment="Left" Margin="320,40,0,0" Width="150" Height="26" VerticalAlignment="Top"/>
