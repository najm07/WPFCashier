<Window x:Class="WPFCashier.JournalsForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFCashier"
        mc:Ignorable="d"
        xmlns:p = "clr-namespace:WPFCashier.Properties"
        Title="{x:Static p:Resources.JournalsFormTitle}" Height="480" Width="800" Loaded="Window_Loaded">
    <Grid>
        <TabControl>
            <TabItem Header="{x:Static p:Resources.ClientsHistory}">
                <Grid>
                    <Grid Margin="5, 10, 5, 0">
                        <Label Content="{x:Static p:Resources.Search}" HorizontalAlignment="Left" Margin="3,0,0,0"   VerticalAlignment="Top"/>
                        <TextBox Name="SearchTextBox" HorizontalAlignment="Left"  Margin="70,0,0,0" Width="250" Height="26" VerticalAlignment="Top" TextChanged="SearchTextBox_TextChanged"/>
                    </Grid>
                    <ListView Name="ItemList" Margin="5,40,5,0" Height="195" VerticalAlignment="Top">
                        <ListView.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Clear" Click="MenuItem_Click"/>
                            </ContextMenu>
                        </ListView.ContextMenu>
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="{x:Static p:Resources.Id}" Width="50"  DisplayMemberBinding="{Binding Id}"/>
                                <GridViewColumn Header="{x:Static p:Resources.Client}" Width="100" DisplayMemberBinding="{Binding DealerName}"/>
                                <GridViewColumn Header="{x:Static p:Resources.Date}" Width="100" DisplayMemberBinding="{Binding Date}"/>
                                <GridViewColumn Header="{x:Static p:Resources.Type}" Width="80" DisplayMemberBinding="{Binding Type}"/>
                                <GridViewColumn Header="{x:Static p:Resources.Receipt}" Width="80" DisplayMemberBinding="{Binding ReceiptNumber}"/>
                                <GridViewColumn Header="{x:Static p:Resources.Amount}" Width="120" DisplayMemberBinding="{Binding Amount}"/>
                                <GridViewColumn Header="{x:Static p:Resources.Old}" Width="120" DisplayMemberBinding="{Binding OldCredit}"/>
                                <GridViewColumn Header="{x:Static p:Resources.New}" Width="120" DisplayMemberBinding="{Binding NewCredit}"/>
                            </GridView>
                        </ListView.View>
                    </ListView>

                    <CheckBox x:Name="EnableCheckBox" Content="{x:Static p:Resources.Edit}"  HorizontalAlignment="Left" Margin="10,240,0,0" VerticalAlignment="Top"/>

                    <Grid x:Name="EditGrid" IsEnabled="{Binding ElementName=EnableCheckBox,Path=IsChecked}" Margin="3,270,0,0"  HorizontalAlignment="Stretch" VerticalAlignment="Top">
                        <Label Content="{x:Static p:Resources.Client}" HorizontalAlignment="Left" Margin="3,0,0,0"   VerticalAlignment="Top"/>
                        <Label Content="{x:Static p:Resources.Date}" HorizontalAlignment="Left" Margin="3,40,0,0" VerticalAlignment="Top"/>
                        <Label Content="{x:Static p:Resources.Type}" HorizontalAlignment="Left" Margin="250,0,0,0" VerticalAlignment="Top"/>
                        <Label Content="{x:Static p:Resources.ReceiptNumber}" HorizontalAlignment="Left" Margin="500,0,0,0" VerticalAlignment="Top"/>
                        <Label Content="{x:Static p:Resources.Amount}" HorizontalAlignment="Left" Margin="250,40,0,0" VerticalAlignment="Top"/>
                        <ComboBox Name="ClientTextBox" Text="{Binding ElementName=ItemList ,Path=SelectedItem.Client,Mode=OneWay}" SelectedValue="{Binding Id}" SelectedValuePath="Id" DisplayMemberPath="Name" HorizontalAlignment="Left"  Margin="70,0,0,0" Width="150" Height="26" VerticalAlignment="Top"/>
                        <DatePicker Name="DateTextBox" Text="{Binding ElementName=ItemList ,Path=SelectedItem.Date,Mode=OneWay}" HorizontalAlignment="Left" Margin="70,40,0,0" Width="150" Height="26" VerticalAlignment="Top"/>
                        <ComboBox Name="TypeTextBox" Text="{Binding ElementName=ItemList ,Path=SelectedItem.Type,Mode=OneWay}" HorizontalAlignment="Left" Margin="320,0,0,0" Width="150" Height="26" VerticalAlignment="Top"/>
                        <TextBox Name="ReceiptTextBox" Text="{Binding ElementName=ItemList ,Path=SelectedItem.Receipt,Mode=OneWay}" HorizontalAlignment="Left" Margin="620,0,0,0" Width="150" Height="26" VerticalAlignment="Top"/>
                        <TextBox Name="AmountTextBox" Text="{Binding ElementName=ItemList ,Path=SelectedItem.Amount,Mode=OneWay}" HorizontalAlignment="Left" Margin="320,40,0,0" Width="150" Height="26" VerticalAlignment="Top"/>
                    </Grid>

